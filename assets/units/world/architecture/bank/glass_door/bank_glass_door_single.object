<?xml version="1.0"?>
<dynamic_object>
  <diesel materials="units/world/architecture/bank/glass_door/bank_glass_doors" orientation_object="rp_glass_officedoor" />
  <sequence_manager file="units/world/architecture/bank/glass_door/bank_glass_door_single" />
  <bodies>
	<body name="door" enabled="true" template="static">
		<object name="rp_glass_officedoor" />
		<object name="g_door" collision_type="mesh_mopp" />
	</body>
	<body remove_ray_type="vis_graph ai_vision" name="window" enabled="true" template="static">
		<object name="g_door_window" collision_type="box" />
	</body>
	<body remove_ray_type="vis_graph ai_vision" name="window_cracked" enabled="false" template="static">
		<object name="g_door_window_cracked" collision_type="convex" />
	</body>
	<body remove_ray_type="walk vis_graph" name="handle_body" enabled="true" template="raycoll_static">
		<object name="c_handle" collision_type="mesh_mopp" />
	</body>
	<body name="dyn_handle_body" enabled="false" template="debris">
		<object name="c_handle_dyn" collision_type="convex" mass="5"/>
	</body>
  </bodies>
  
  <decal_surfaces default_material="glass_breakable">
    <decal_mesh name="g_door" enabled="true" material="metal" />
    <decal_mesh name="g_handle" enabled="true" material="metal" />
    <decal_mesh name="g_door_window" enabled="true" material="glass_breakable" />
    <decal_mesh name="g_door_window_cracked" enabled="false" material="glass_breakable" />
  </decal_surfaces>
  
  <graphics>
    <object name="g_door" enabled="true" />
    <object name="g_door_window" enabled="true" />
    <object name="g_handle" enabled="true" />
    <object name="g_door_window_cracked" enabled="false" />
  </graphics>
</dynamic_object>